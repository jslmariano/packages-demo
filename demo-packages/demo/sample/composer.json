{
    "name": "demo/sample",
    "description": "Just a custom sample package for demo",
    "type": "library, lumen",
    "version": "0.1.0",
    "keywords": [
        "lumen",
        "demo",
        "sample"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Demo\\Sample\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Demo\\Sample\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
